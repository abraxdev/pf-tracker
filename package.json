{
  "name": "personal-finance-tracker",
  "version": "1.0.0",
  "description": "Web application for personal finance management with multiple bank account aggregation",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon app.js",
    "tailwind:build": "tailwindcss -i ./public/css/input.css -o ./public/css/styles.css",
    "tailwind:watch": "tailwindcss -i ./public/css/input.css -o ./public/css/styles.css --watch",
    "extract-isin": "node ../scripts/extract-isin.js",
    "resolve-isin": "NODE_PATH=node_modules node ../scripts/resolve-isin.js",
    "resolve-isin:batch": "NODE_PATH=node_modules node ../scripts/resolve-isin.js --batch",
    "ticker-status": "NODE_PATH=node_modules node ../scripts/check-ticker-status.js",
    "build-pf": "NODE_PATH=node_modules node ../scripts/build-pf-position.js",
    "search-figi": "node ../scripts/search-figi.js",
    "scrape-pf-titles": "NODE_PATH=node_modules node ../scripts/scrape-pf-title.js"
  },
  "keywords": [
    "finance",
    "banking",
    "tracker",
    "supabase",
    "ai-classification"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@supabase/supabase-js": "^2.39.0",
    "apexcharts": "^5.3.6",
    "axios": "^1.6.2",
    "csv-parser": "^3.0.0",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "form-data": "^4.0.0",
    "hbs": "^4.2.0",
    "iconv-lite": "^0.6.3",
    "multer": "^1.4.5-lts.1",
    "pdf-parse": "^2.4.5",
    "puppeteer": "^24.34.0",
    "xlsx": "^0.18.5",
    "yahoo-finance2": "^3.10.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "tailwindcss": "^3.3.6"
  }
}
